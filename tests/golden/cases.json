    "html": "<h1>Title</h1>",
    "expected": "\\section{Title}"
  },
  {
    "name": "br_in_p",
    "html": "<p>Hello<br/>World</p>",
    "expected": "Hello\\newline World\\par "
  },
  {
    "name": "ul_li",
    "html": "<ul><li>One</li><li>Two</li></ul>",
    "expected": "\\begin{itemize}\\item One\\item Two\\end{itemize}"
  },
  {
    "name": "a_link",
    "html": "<a href=\"https://example.com\">Example</a>",
    "expected": "\\href{https://example.com}{Example}"
  },
  {
    "name": "blockquote",
    "html": "<blockquote>Quoted text</blockquote>",
    "expected": "\\begin{quote}Quoted text\\end{quote}"
  },
  {
    "name": "pre_block",
    "html": "<pre>print('hi')</pre>",
    "expected": "\\begin{verbatim}print('hi')\\end{verbatim}"
  },
  {
    "name": "inline_code",
    "html": "<p>Use <code>foo()</code></p>",
    "expected": "Use \\texttt{foo()}\\par "
  },
  {
    "name": "h5_header",
    "html": "<h5>Small</h5>",
    "expected": "Small"
  },
  {
    "name": "strike_text",
    "html": "<del>gone</del>",
    "expected": "gone"
  },
  {
    "name": "span_bold",
    "html": "<span style=\"font-weight: bold;\">Bold</span>",
    "expected": "Bold"
  },
  {
    "name": "span_color",
    "html": "<span style=\"color: #ff0000;\">Red</span>",
    "expected": "Red"
  }
