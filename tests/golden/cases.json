[
  {
    "name": "p_simple",
    "html": "<p>Hello World</p>",
    "expected": "\\noindent Hello World\\par"
  },
  {
    "name": "h1_simple",
    "html": "<h1>Title</h1>",
    "expected": "\\section{Title}"
  },
  {
    "name": "br_in_p",
    "html": "<p>Hello<br/>World</p>",
    "expected": "\\noindent Hello\\newline World\\par"
  },
  {
    "name": "ul_li",
    "html": "<ul><li>One</li><li>Two</li></ul>",
    "expected": "\\begin{itemize}[noitemsep,nosep]\\begin{item}One\\end{item}\\begin{item}Two\\end{item}\\end{itemize}"
  },
  {
    "name": "a_link",
    "html": "<a href=\"https://example.com\">Example</a>",
    "expected": "\\href{https://example.com}{Example}"
  },
  {
    "name": "blockquote",
    "html": "<blockquote>Quoted text</blockquote>",
    "expected": "\\begin{quote}\nQuoted text\n\\end{quote}"
  },
  {
    "name": "pre_block",
    "html": "<pre>print('hi')</pre>",
    "expected": "\\begingroup\n\\ttfamily\n\\detokenize{print('hi')}\n\\par\n\\endgroup"
  },
  {
    "name": "inline_code",
    "html": "<p>Use <code>foo()</code></p>",
    "expected": "\\noindent Use\\texttt{foo()}\\par"
  },
  {
    "name": "h5_header",
    "html": "<h5>Small</h5>",
    "expected": "\\subparagraph{Small}"
  },
  {
    "name": "strike_text",
    "html": "<del>gone</del>",
    "expected": "\\sout{gone}"
  },
  {
    "name": "span_bold",
    "html": "<span style=\"font-weight: bold;\">Bold</span>",
    "expected": "\\textbf{Bold}"
  },
  {
    "name": "span_color",
    "html": "<span style=\"color: #ff0000;\">Red</span>",
    "expected": "\\textcolor[HTML]{FF0000}{Red}"
  }
]
